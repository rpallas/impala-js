{
  "name": "@rpallas/impala-js",
  "version": "1.4.0",
  "description": "Javascript wrapper for Impala hotel PMS API",
  "main": "src/index.js",
  "scripts": {
    "test": "npx jest --silent --detectOpenHandles",
    "test-spec": "npx jest --silent --verbose --detectOpenHandles",
    "test-watch": "npx jest --watch --coverage --verbose --silent",
    "cov": "npx jest --coverage --silent",
    "cov-open": "npx jest --coverage --silent; open coverage/lcov-report/index.html",
    "majestic": "npx majestic",
    "generate-docs": "npx jsdoc -c jsdoc.json"
  },
  "keywords": [
    "impala",
    "javascript",
    "client",
    "sdk"
  ],
  "author": "Robbie Pallas",
  "license": "ISC",
  "devDependencies": {
    "jest": "26.0.0"
  },
  "dependencies": {
    "got": "^10.0.0",
    "moment": "^2.24.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/rpallas/impala-js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
